@{
    ViewData["Title"] = "Results";
}
@model Survey

<div>
    <h1 class="mt-4">Submitted Info</h1>
    <p class="mt-3">Name: @Model.name</p>
    <p class="mt-3">Dojo Location: @Model.location</p>
    <p class="mt-3">Favorite Language: @Model.language</p>
    <p class="mt-3">Comments: @Model.comment</p>
    <a href="/" class="btn btn-info">Back Home</a>
</div>